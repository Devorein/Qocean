{
  "name": "qocean",
  "version": "1.0.0",
  "description": "MERN App for creating awesome quizzes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com:Devorein/Qocean.git"
  },
  "main": "index.js",
  "keywords": [],
  "author": "Devon Reid <devorein00@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "node ./utils/linkParentBin.js",
    "client": "cd ./client && npm start",
    "start": "concurrently -c green,blue --names \"server,client\" \"npm run server\" \"npm run client\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npx eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/register": "^7.10.5",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.2.1",
    "link-parent-bin": "^1.0.2",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-rc.12",
    "axios": "^0.19.2",
    "colors": "^1.4.0",
    "fs-extra": "^9.0.1",
    "graphql": "^15.3.0",
    "lodash": "^4.17.19",
    "mkdirp": "^1.0.4",
    "move-file": "^2.0.0",
    "rimraf": "^3.0.2",
    "lint-staged": "^10.2.11"
  }
}